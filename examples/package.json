    "test data",
    "Google Sheets",
    "Google Docs",
    "sheets",
    "cli"
  ],
  "devDependencies": {
    "@types/eslint": "^8.4.6",
    "@types/jest": "~27.5",
    "@types/node": "~16",
    "@typescript-eslint/eslint-plugin": "~5.26",
    "@typescript-eslint/parser": "~5.26",
    "bitloops-gherkin": "^0.2.5",
    "env-cmd": "^10.1.0",
    "eslint": "~8.16",
